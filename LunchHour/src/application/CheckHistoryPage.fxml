            <Label text="Starting Date:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label text="End Date:" GridPane.rowIndex="1">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="searchid" GridPane.columnIndex="1" GridPane.rowIndex="2" />
