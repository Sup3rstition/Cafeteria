<AnchorPane prefHeight="628.0" prefWidth="632.0" style="-fx-background-color: #DDECDD;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CheckHistoryController">
